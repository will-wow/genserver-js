{
  "name": "genserver-js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^23.3.2",
    "babel-eslint": "^7.2.3",
    "classnames": "^2.2.6",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.1",
    "husky": "^0.14.3",
    "jest-junit": "^5.1.0",
    "lint-staged": "^7.2.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3"
  },
  "scripts": {
    "format": "npm-run-all -c lint:*",
    "lint:eslint": "eslint 'src/**/*.{js,jsx}'",
    "lint:prettier": "prettier --write **/*.{js,jsx,ts,tsx,css,scss,json,md}",
    "lint:stylelint": "stylelint src/**/*.scss",
    "lint:tslint": "tslint 'src/**/*.{ts,tsx}'",
    "test": "react-scripts-ts test --env=jsdom",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "eslint",
    "src/**/*.{ts,tsx}": "tslint"
  },
  "engines": {
    "node": "8.11.3",
    "yarn": "1.12.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  }
}
